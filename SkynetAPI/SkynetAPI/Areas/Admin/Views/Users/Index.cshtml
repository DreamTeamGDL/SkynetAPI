@model List<User>;

<div class="wrapper-title col-xs-12 col-sm-12 col-md-12">
    <img src="~/img/logo/logo-wd.png" />
    <h1>Welcome to Web Admin</h1>
</div>
<div class="wrapper-header-content col-md-12">
    <div class="col-md-4">
        <h1>Users</h1>
    </div>
</div>
<div class="wrapper-table">
    <div class="row">
        <div class="col-lg-12">
            <table class="table">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Username</th>
                        <th>Id</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var user in Model)
                    {
                        <tr>
                            <td><a class="row-name" asp-area="admin" asp-controller="zones" asp-route-id="@user.Id">@user.Name</a></td>
                            <td>@user.Username</td>
                            <td>@user.Id</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>